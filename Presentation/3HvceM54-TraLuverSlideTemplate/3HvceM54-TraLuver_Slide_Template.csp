PresentationConfig.addSlideTemplate("TraLuver");</pre></div><div class="paragraph style-paragraph collapsed" data-id="Yv92JHFr" name="TraLuver Slide Style" style=""><style data-type="content" type="text/css" codemirror="true">html .paragraph .slide[slide-template="TraLuver"][data-type="content"],
html .paragraph .slide[slide-template="TraLuver"] .slide-content {
  margin: 0;
  padding: 0;
  overflow: hidden;
  font-family: "Roboto", "Avenir", sans-serif;
  font-weight: 400;
}

/******************************************************************************/

html .paragraph .slide[slide-template="TraLuver"] .slide-content ul,
html .paragraph .slide[slide-template="TraLuver"] .slide-content ol {
  margin-left: 1rem;
}

/******************************************************************************/

html .slide[slide-template="TraLuver"] .slide-content {
  background: var(--material-color-white);
  color: var(--material-color-black);
  font-size: 24px;
  display: flex;
  flex-direction: column;
}

html .slide[slide-template="TraLuver"] .slide-content h1 {
  font-size: 96px;
}</style></div><div class="paragraph style-paragraph collapsed" data-id="zXb5uycG" name="TraLuver Components" style="" draggable="false"><style data-type="content" type="text/css" codemirror="true" class="themable persistent">/* Hack for a margin without increasing the size of the element */
html .slide[slide-template="TraLuver"] .slide-content {
  box-sizing: border-box !important;
  border: 48px solid rgba(0, 0, 0, 0);
}

/******************************************************************************/

html .slide[slide-template="TraLuver"] .slide-content .no-padding {
  padding: 0 !important;
}

html .slide[slide-template="TraLuver"] .slide-content .no-margin {
  margin: 0 !important;
}

html .slide[slide-template="TraLuver"] .slide-content .center,
html .slide[slide-template="TraLuver"] .slide-content .middle,
html .slide[slide-template="TraLuver"] .slide-content .left,
html .slide[slide-template="TraLuver"] .slide-content .right,
html .slide[slide-template="TraLuver"] .slide-content .top,
html .slide[slide-template="TraLuver"] .slide-content .bottom {
  display: flex;
  flex-direction: column;
}

html .slide[slide-template="TraLuver"] .slide-content .center {
  align-items: center;
}

html .slide[slide-template="TraLuver"] .slide-content .left {
  align-items: flex-start;
}

html .slide[slide-template="TraLuver"] .slide-content .right {
  align-items: flex-end;
}

html .slide[slide-template="TraLuver"] .slide-content .middle {
  justify-content: center;
  height: 100%;
}

html .slide[slide-template="TraLuver"] .slide-content .top {
  justify-content: flex-start;
  height: 100%;
}

html .slide[slide-template="TraLuver"] .slide-content .bottom {
  justify-content: flex-end;
  height: 100%;
}

html .slide[slide-template="TraLuver"] .slide-content .middle-self {
  justify-self: center;
}

/******************************************************************************/

html .slide[slide-template="TraLuver"] .slide-content .split-h {
  display: flex;
  flex-direction: row;
  height: 100%;
}

html .slide[slide-template="TraLuver"] .slide-content .split-v {
  display: flex;
  flex-direction: column;
  height: 100%;
}

html .slide[slide-template="TraLuver"] .slide-content .split-h > *,
html .slide[slide-template="TraLuver"] .slide-content .split-v > * {
  flex: 1 1 50%;
  margin: 0 !important;
}

html .slide[slide-template="TraLuver"] .slide-content .split-h > * + * {
  margin-left: 32px !important;
}

html .slide[slide-template="TraLuver"] .slide-content .split-v > * + * {
  margin-top: 32px !important;
}

/******************************************************************************/

html .slide[slide-template="TraLuver"] .slide-content slide-content,
html .slide[slide-template="TraLuver"] .slide-content render-to,
html .slide[slide-template="TraLuver"] .slide-content render-to > transient,
html .slide[slide-template="TraLuver"] .slide-content iframe {
  width: 100%;
  height: 100%;
}

/******************************************************************************/

html .slide[slide-template="TraLuver"] .slide-content h1,
html .slide[slide-template="TraLuver"] .slide-content h2,
html .slide[slide-template="TraLuver"] .slide-content h3 {
  margin: 0;
}

/* Title */
html .slide[slide-template="TraLuver"] .slide-content h1:first-of-type {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%) translateY(-48px);
  width: 100%;
  text-align: center;
}

/* Slide Header */
html .slide[slide-template="TraLuver"] .slide-content h2:first-of-type {
  margin-bottom: 96px;
}

/* Subtitle */
html .slide[slide-template="TraLuver"] .slide-content h3:first-of-type {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%) translateY(48px);
  width: 100%;
  text-align: center;
}

html .slide[slide-template="TraLuver"] .slide-content a {
  color: orange !important;
}

html .slide[slide-template="TraLuver"] .slide-content .keyword {
