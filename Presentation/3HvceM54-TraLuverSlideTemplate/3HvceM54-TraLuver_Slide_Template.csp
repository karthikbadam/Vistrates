}

html .slide[slide-template="TraLuver"] .slide-content .topleft-box,
html .slide[slide-template="TraLuver"] .slide-content .topright-box,
html .slide[slide-template="TraLuver"] .slide-content .bottomleft-box,
html .slide[slide-template="TraLuver"] .slide-content .bottomright-box {
