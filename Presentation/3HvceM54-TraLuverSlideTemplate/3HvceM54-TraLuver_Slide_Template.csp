html .slide[slide-template="TraLuver"] .slide-content .topleft-box .copyright,
html .slide[slide-template="TraLuver"] .slide-content .topright-box .copyright,
html .slide[slide-template="TraLuver"] .slide-content .bottomleft-box .copyright,
html .slide[slide-template="TraLuver"] .slide-content .bottomright-box .copyright {
  margin-bottom: 25px;
  max-width: 90%;
}

html .slide[slide-template="TraLuver"] .slide-content .topleft-box {
  top: 20px;
